{
  "name": "electrolux-source",
  "version": "1.0.0",
  "description": "Electrolux Chatbot for practice. IBM-GBS",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test-integration": "casperjs test ./test/integration/test.*.js",
    "test-integration-runner": "NODE_ENV=test node casper-runner.js",
    "test": "npm run lint && npm run test-unit && npm run autofix",
    "test-unit": "istanbul cover ./node_modules/mocha/bin/_mocha test/unit",
    "lint": "eslint .",
    "autofix": "eslint --fix .",
    "codecov": "npm run test && (codecov || true)"
  },
  "keywords": [
    "chatbot",
    "electrolux"
  ],
  "author": "Rishabh Chakrabarti",
  "repository": {
    "type": "git",
    "url": "https://github.com/bassdeveloper/Electrolux-Demo.git"
  },
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.16.0",
    "cf-blue-green": "^0.2.2",
    "cookie-parser": "^1.4.3",
    "dot-env": "0.0.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "multer": "^1.2.1",
    "watson-developer-cloud": "^2.15.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "casperjs": "^1.1.3",
    "codecov": "^1.0.1",
    "eslint": "^3.14.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "phantomjs-prebuilt": "^2.1.14",
    "supertest": "^2.0.1"
  }
}
